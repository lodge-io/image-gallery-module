{
  "name": "Lodge.io",
  "version": "1.0.0",
  "description": "Front-end Capstone Project",
  "author": "",
  "license": "",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "dev:react": "webpack -d --watch",
    "start:dev": "nodemon server/app.js",
    "start": "node seed.js && node server/",
    "test": "jest --watch",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cors": "^2.8.4",
    "babel-jest": "^23.4.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "mongoose": "^5.2.4",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.4.1",
    "react-fontawesome": "^1.6.1",
    "react-slick": "^0.23.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-hackreactor": "github:reactorcore/eslint-config-hackreactor",
    "jest": "^23.4.1",
    "react-test-renderer": "^16.4.1",
    "style-loader": "^0.21.0",
    "superagent": "^3.8.2",
    "supertest": "^3.0.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/client/src/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/client/src/setup/"
    ],
    "collectCoverage": true,
    "verbose": true,
    "moduleFileExtensions": [
      "jsx",
      "js"
    ],
    "testMatch": [
      "**/client/src/components/**/*.test.{js,jsx}",
      "**/__tests__/*.{js}"
    ]
  }
}
